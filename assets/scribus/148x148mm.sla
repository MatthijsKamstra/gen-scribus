            <StoryText>
                <DefaultStyle PARENT="Default Paragraph Style" LINESPMode="1" CPARENT="Default Character Style"/>
                <ITEXT CH="Piramide van Maslow"/>
                <para PARENT="Text3_Heading 1"/>
                <ITEXT CH="De (behoefte)piramide werd ontwikkeld door de Amerikaanse psycholoog "/>
                <ITEXT CPARENT="Text5_Bold" CH="Abraham Maslow"/>
                <ITEXT CH=". Volgens de theorie van Maslow kan pas aan hogere behoeften worden toegekomen als aan de onderliggende (en lagere) behoeften is voldaan."/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CH="Behoeftes"/>
                <para PARENT="Text3_Heading 2"/>
                <ITEXT CPARENT="Text5_Bold" CH="Biologische en lichamelijke behoeften:"/>
                <ITEXT CH=" slaap, voedsel, drinken"/>
                <para PARENT="Text5_Numbered List 1"/>
                <ITEXT CPARENT="Text5_Bold" CH="Veiligheid en zekerheid:"/>
                <ITEXT CH=" persoonlijke, emotionele, financiële"/>
                <para PARENT="Text5_Numbered List 1"/>
                <ITEXT CPARENT="Text5_Bold" CH="Sociaal contact:"/>
                <ITEXT CH=" vriendschap, liefde en positief-sociale relaties"/>
                <para PARENT="Text5_Numbered List 1"/>
                <ITEXT CPARENT="Text5_Bold" CH="Waardering, erkenning en zelfrespect:"/>
                <ITEXT CH=" status in sociaal verband"/>
                <para PARENT="Text5_Numbered List 1"/>
                <ITEXT CPARENT="Text5_Bold" CH="Zelfontplooiing:"/>
                <ITEXT CH=" tijd en geld hebt om jezelf te ontwikkelen"/>
                <para PARENT="Text5_Numbered List 1"/>
                <ITEXT CH="Kritiek"/>
                <para PARENT="Text3_Heading 2"/>
                <ITEXT CH="“My motivation theory was published 20 years ago, and in all that time nobody repeated it, or tested it, or really analysed it or criticised it. They just used it, swallowed it whole with only the most minor modifications...” "/>
                <ITEXT CPARENT="Text5_BoldItalic" CH="– Abraham Maslow"/>
                <para/>
            </StoryText>
